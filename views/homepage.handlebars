<div class="posts">
  {{#each blogs as |blogpost|}};
    <div class="card">
      <div class="card-header">
        <h2>{{blogpost.title}}</h2>
        <h3>{{blogpost.authorId}}</h3>
      </div>
      <article>{{blogpost.content}}</article>
    </div>
  {{/each}}
</div>

